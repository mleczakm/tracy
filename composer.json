{
    "description": "TRAcking with privaCY",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "ext-ds": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-swoole": "*",
        "flow-php/etl": "^0.5.2",
        "flow-php/etl-adapter-xml": "^0.5.2",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
        }
    }
}
